{
  "name": "leaflet-drag-and-drop",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel-node": "babel-node --presets=es2015",
    "deploy": "gulp deploy --production",
    "dev": "gulp & nodemon --exec npm run babel-node -- server.js"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react",
      "html"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^[A-Z].*"
        }
      ]
    },
    "globals": {
      "document": true,
      "window": true
    }
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.0",
    "browserify-css": "^0.9.2",
    "envify": "^3.4.1",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-html": "^1.5.3",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp-util": "^3.0.7",
    "http": "0.0.0",
    "jquery": "^3.1.1",
    "js-logger": "^1.3.0",
    "leaflet": "^1.0.1",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "reactify": "^1.1.1",
    "reload": "^1.0.1",
    "uglifyify": "^3.0.4",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
